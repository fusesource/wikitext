<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         id="org.eclipse.mylar.hypertext.context"
         point="org.eclipse.mylar.core.context">
      <structureBridge
            class="org.eclipse.mylar.hypertext.HypertextStructureBridge"
            name="Hypertext Structure Bridge"/>
   </extension>
   <extension
         point="org.eclipse.mylar.ui.context">
      <uiBridge
            class="org.eclipse.mylar.hypertext.HypertextUiBridge"
            contentType="html"
            id="org.eclipse.mylar.hypertext.uiBridge"/>
   </extension>
   <extension
         point="org.eclipse.mylar.tasklist.editors">
      <editorFactory class="org.eclipse.mylar.hypertext.ui.editors.WebElementsEditorFactory"/>
   </extension>
</plugin>
